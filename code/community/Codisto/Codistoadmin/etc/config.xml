 <config>
	<modules>
		<Codisto_Codistoadmin>
			<version>0.1.0</version>
			<depends></depends>
		</Codisto_Codistoadmin>
	</modules>
	<global>
		<helpers>
			<Codisto_Codistoadmin>
				<class>Codisto_Codistoadmin_Helper</class>
			</Codisto_Codistoadmin>
