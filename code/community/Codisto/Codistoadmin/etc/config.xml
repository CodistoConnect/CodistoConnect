	</global>
	<admin>
		<routers>
			<adminhtml>
				<args>
					<modules>
						<Codisto_Codistoadmin after="Mage_Adminhtml">Codisto_Codistoadmin</Codisto_Codistoadmin>
					</modules>
				</args>
			</adminhtml>
		</routers>
	</admin>
